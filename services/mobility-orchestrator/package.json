{
  "name": "@easymo/mobility-orchestrator",
  "version": "1.0.0",
  "description": "Workflow orchestrator for mobility trip matching",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@easymo/commons": "workspace:*",
    "@supabase/supabase-js": "^2.86.2",
    "axios": "^1.13.1",
    "express": "^4.21.2",
    "zod": "^3.25.76",
    "pino": "^9.5.0",
    "pino-http": "^9.0.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.18.12",
    "eslint": "^9.18.0",
    "tsx": "^4.19.2",
    "typescript": "5.5.4",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
