{
  "name": "@easymo/ai-realtime",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "test": "jest --runInBand",
    "ai:persona:cfo": "curl -sS -X POST http://localhost:7070/ai/persona -H 'content-type: application/json' -d '{\"key\":\"cfo\"}'",
    "ai:demo": "curl -sS -X POST http://localhost:7070/ai/say -H 'content-type: application/json' -d '{\"text\":\"Hi!\"}'"
  },
  "dependencies": {
    "@easymo/commons": "workspace:*",
    "@supabase/supabase-js": "^2.76.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "pino": "^9.5.0",
    "ws": "^8.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.12",
    "@types/ws": "^8.5.13",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
