{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@va/shared": [
        "packages/shared/src/index.ts"
      ],
      "@va/shared/*": [
        "packages/shared/src/*"
      ],
      "@easymo/video-agent-schema": [
        "packages/video-agent-schema/dist/index"
      ],
      "@easymo/video-agent-schema/*": [
        "packages/video-agent-schema/dist/*"
      ],
      "@easymo/commons": [
        "packages/commons/src/index.ts"
      ],
      "@easymo/commons/*": [
        "packages/commons/src/*"
      ]
    },
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "jsx": "react-jsx"
  },
  "files": []
}