{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@va/shared": ["packages/shared/src/index.ts"],
      "@va/shared/*": ["packages/shared/src/*"],
      "@easymo/video-agent-schema": ["packages/video-agent-schema/dist/index"],
      "@easymo/video-agent-schema/*": ["packages/video-agent-schema/dist/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "jsx": "react-jsx"
  },
  "files": []
}
