# Marketing Web (Vite) environment template
# Public values (exposed in browser)
VITE_SUPABASE_URL=https://<project-ref>.supabase.co
VITE_SUPABASE_ANON_KEY=<anon-key>
VITE_API_BASE=/functions/v1
VITE_USE_MOCK=false
VITE_ENABLE_AGENT_CHAT=false
VITE_ADMIN_TOKEN=<optional-dev-token>
VITE_MOBILITY_USER_ID=00000000-0000-4000-8000-000000000001
VITE_MOBILITY_USER_ROLES=admin

# Local-only overrides (never set in production browser builds)
VITE_SUPABASE_SERVICE_ROLE_KEY=<local-only-service-role>
