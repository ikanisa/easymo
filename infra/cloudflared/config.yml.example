# Replace with the Cloudflare Tunnel name configured for this environment.
tunnel: your-tunnel-name

# Path to the credentials file generated by `cloudflared tunnel create`.
# Update this placeholder with the actual credentials file location.
credentials-file: /etc/cloudflared/your-tunnel-credentials.json

ingress:
  # Routes requests for the admin UI through the tunnel.
  - hostname: ${ADMIN_HOSTNAME}
    service: http://localhost:8080
  # Fallback rule required by cloudflared to handle unmatched requests.
  - service: http_status:404
