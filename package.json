    "openai": "^4.104.0",
    "pino": "^9.14.0",
    "pino-pretty": "^11.3.0",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-resizable-panels": "^2.1.9",
