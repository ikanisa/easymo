    "@types/qrcode": "^1.5.5",
    "@va/shared": "workspace:*",
    "@vitest/ui": "^3.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "jsdom": "^26.1.0",
