{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8080",
    "start": "node ./scripts/start-preview.mjs",
    "prebuild": "node ./scripts/assert-no-service-role-in-client.mjs",
    "build": "pnpm --filter @va/shared build && vite build",
    "build:dev": "vite build --mode development",
    "format": "prettier --write \"docs/{env,standards}.md\" \"packages/config/**/*.{js,ts,json}\" \"packages/utils/log/**/*.{js,ts,json}\" \"eslint.config.js\" \"prettier.config.js\" \"tsconfig.strict.json\"",
    "format:check": "prettier --check \"docs/{env,standards}.md\" \"packages/config/**/*.{js,ts,json}\" \"packages/utils/log/**/*.{js,ts,json}\" \"eslint.config.js\" \"prettier.config.js\" \"tsconfig.strict.json\"",
    "lint": "eslint . && pnpm --filter @easymo/admin-app run lint",
    "type-check": "tsc --noEmit && pnpm exec tsc --noEmit -p apps/api/tsconfig.json",
    "test": "pnpm schema:verify && vitest run && pnpm --filter @easymo/log run test && pnpm --filter @easymo/admin-app exec vitest run --config vitest.config.smoke.cjs && pnpm --filter voice-api run test",
    "test:functions": "deno test --allow-env --allow-read --allow-net supabase/functions/tests",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "preview": "vite preview --port 5000",
    "schema:verify": "node ./scripts/check-schema-alignment.mjs",
    "analyze:pwa": "lighthouse http://localhost:5000 --only-categories=pwa --preset=desktop || true",
    "health": "node ./scripts/health-check.mjs",
    "seed:remote": "bash ./scripts/seed-remote.sh",
    "functions:deploy": "supabase functions deploy admin-settings admin-stats admin-users admin-trips admin-subscriptions simulator",
    "embeddings:sync": "node ./scripts/embeddings/ingest-documents.mjs",
    "perf:seed": "node ./tests/perf/seed/perf_seed.mjs",
    "perf:test:voucher": "k6 run tests/perf/k6/voucher_preview_spike.js",
    "perf:test:redeem": "k6 run tests/perf/k6/station_redeem_peak.js",
    "perf:test:campaign": "locust -f tests/perf/locust/campaign_bulk_send.py",
    "test:sql": "pg_prove --failures --recurse tests/sql",
    "test:newman:flow-exchange": "newman run tests/postman/flow-exchange.postman.json --env-var FLOW_EXCHANGE_URL=$FLOW_EXCHANGE_URL",
    "coverage:vitest": "vitest run --coverage",
    "coverage:api": "pnpm --filter voice-api exec jest --coverage",
    "coverage:merge": "node ./scripts/merge-coverage.mjs",
    "coverage": "pnpm coverage:vitest && pnpm coverage:api && pnpm coverage:merge"
  },
  "dependencies": {
    "@easymo/config": "workspace:*",
