{
  "connectors": {
    "supabase": {
      "type": "database",
      "url": "${SUPABASE_URL}",
      "anonKey": "${SUPABASE_ANON_KEY}",
      "serviceRoleKey": "${SUPABASE_SERVICE_ROLE_KEY}",
      "description": "Primary Supabase database for vouchers and customers"
    },
    "whatsapp": {
      "type": "messaging",
      "phoneNumberId": "${WA_PHONE_ID}",
      "accessToken": "${WA_TOKEN}",
      "verifyToken": "${WA_VERIFY_TOKEN}",
      "apiBase": "${WHATSAPP_API_URL}",
      "description": "WhatsApp Cloud API for messaging"
    },
    "sip": {
      "type": "telephony",
      "trunkUri": "${SIP_TRUNK_URI}",
      "authUser": "${SIP_AUTH_USER}",
      "authPass": "${SIP_AUTH_PASS}",
      "description": "SIP trunk for voice calls"
    },
    "openai": {
      "type": "ai",
      "apiKey": "${OPENAI_API_KEY}",
      "responsesModel": "${OPENAI_RESPONSES_MODEL}",
      "realtimeModel": "${OPENAI_REALTIME_MODEL}",
      "description": "OpenAI APIs for agent intelligence"
    }
  },
  "metadata": {
    "version": "1.0.0",
    "description": "AgentKit connector registry for EasyMO integrations"
  }
}
